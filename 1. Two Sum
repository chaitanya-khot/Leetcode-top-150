class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        pairs, ans = {}, []
        for num in range(len(nums)):
            if len(pairs) <= len(nums):
                pairs[nums[num]] = target - nums[num]
    
        for num in pairs.values():
            if num in nums and pairs[num] != num:
                ans.append(nums.index(num))
        return ans[::-1]
